@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

list(APPEND CMAKE_MODULE_PATH ${CMAKE_CURRENT_LIST_DIR}/../Modules)

find_dependency(SC REQUIRED)
find_dependency(P4EST REQUIRED)
find_dependency(ZLIB)

include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")

# set component found flags
if(TARGET @PROJECT_NAME@::CLAWPATCH)
    set(@PROJECT_NAME@_CLAWPATCH_FOUND TRUE)
endif()
if(TARGET @PROJECT_NAME@::CLAWPACK4.6)
    set(@PROJECT_NAME@_CLAWPACK4.6_FOUND TRUE)
endif()
if(TARGET @PROJECT_NAME@::CLAWPACK5)
    set(@PROJECT_NAME@_CLAWPACK5_FOUND TRUE)
endif()
if(TARGET @PROJECT_NAME@::CLAWPACK5_3D)
    set(@PROJECT_NAME@_CLAWPACK5_3D_FOUND TRUE)
endif()
if(TARGET @PROJECT_NAME@::CLAWPACK3_46)
    set(@PROJECT_NAME@_CLAWPACK3_46_FOUND TRUE)
endif()
if(TARGET @PROJECT_NAME@::CLAWPACK3_46)
    set(@PROJECT_NAME@_CLAWPACK3_46_FOUND TRUE)
endif()
if(TARGET @PROJECT_NAME@::FC2D_THUNDEREGG)
    set(@PROJECT_NAME@_FC2D_THUNDEREGG_FOUND TRUE)
    find_dependency(ThunderEgg)
    if(NOT ThunderEgg_FOUND)
        set(@PROJECT_NAME@_FC2D_THUNDEREGG_FOUND FALSE)
    endif()
endif()
if(TARGET @PROJECT_NAME@::DUMMY)
    set(@PROJECT_NAME@_DUMMY_FOUND TRUE)
endif()
if(TARGET @PROJECT_NAME@::CUDACLAW)
    set(@PROJECT_NAME@_CUDACLAW_FOUND TRUE)
    set_target_properties(@PROJECT_NAME@::CUDACLAW PROPERTIES CUDA_SEPARABLE_COMPILATION ON)

    find_dependency(CUDAToolkit)
    if(NOT CUDAToolkit_FOUND)
        set(@PROJECT_NAME@_CUDACLAW_FOUND FALSE)
    endif()
endif()

check_required_components(@PROJECT_NAME@)
